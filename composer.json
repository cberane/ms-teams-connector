{
    "name": "cberane/ms-teams-connector",
    "description": "A simple package to send messages through incoming webhooks to Microsoft Teams",
    "keywords": [
        "cberane",
        "Microsoft Teams",
        "Connector",
        "Webhook"
    ],
    "homepage": "https://github.com/cberane/ms-teams-connector",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Cristoph Berane",
            "email": "cristoph@berane.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cberane\\MsTeamsConnector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cberane\\MsTeamsConnector\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-interactive": "vendor/bin/phpunit --testsuite Interactive",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "test-full-coverage": "vendor/bin/phpunit --coverage-html coverage --testsuite Feature,Unit,Interactive"
    },
    "config": {
        "sort-packages": true
    }
}
